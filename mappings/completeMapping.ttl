thomas:EmployeeTripleMap
    a rr:TriplesMap ;

    rr:logicalTable [rr:sqlQuery """select replace(FullName, ' ', '') as ID, * 
                                    from webshop.Employee"""] ;
    rr:subjectMap [
        rr:template "http://www.example.com/thomas#employee_{ID}";
